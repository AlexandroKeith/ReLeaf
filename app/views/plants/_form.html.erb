<%= simple_form_for(@plant) do |f| %>

  <div class="form-inputs">
    <%= f.input :species %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <%= f.input :photos, as: :file, input_html: { multiple: true } %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class:'btn btn-primary'  %>
  </div>
<% end %>
